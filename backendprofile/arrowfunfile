class student{
     constructor(name, age, marks){
        this.name=name;
        this.age=age;
        this.marks=marks;
     }
    studentreport(marks){
        if(this.marks<50){
            console.log(this.name+" cleared");
        }else{
            console.log(this.name+" not cleared");
        }

     }
}
const jhon= new student('jhon', 18, 68);
const shri=new student('shri', 20, 45);
jhon.studentreport(48);
shri.studentreport(89);

const mul=(a,b)=> a*b;
console.log(mul(3,10));

